<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Absensi November</title>
        <link
            rel="shortcut icon"
            href="./assets/compiled/svg/favicon.svg"
            type="image/x-icon"
        />
        <link
            rel="shortcut icon"
            href="data:image/png;base64,"
            type="image/png"
        />

        <link rel="stylesheet" href="/assets/compiled/css/app.css" />
        <link rel="stylesheet" href="/assets/compiled/css/app-dark.css" />
    </head>

    <body>
        <script src="/assets/static/js/initTheme.js"></script>
        <script>
            function formatAngka(angka) {
                return angka.toString().replace(/\B(?=(\d{3})+(?!\d))/g, '.');
            }
        </script>
        <div id="app">
            <%- include('partials/navbar.ejs') %>

            <div id="main">
                <div class="page-heading">
                    <div class="page-title">
                        <div class="row">
                            <div class="col-12 col-md-6 order-md-1 order-last">
                                <h3><%= namaBulan %> - <%= nama %></h3>
                            </div>
                            <div class="col-12 col-md-6 order-md-2 order-first">
                                <nav
                                    aria-label="breadcrumb"
                                    class="breadcrumb-header float-start float-lg-end"
                                >
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item">
                                            <a href="index.html">Dashboard</a>
                                        </li>
                                        <li
                                            class="breadcrumb-item active"
                                            aria-current="page"
                                        >
                                            Payroll
                                        </li>
                                        <li
                                            class="breadcrumb-item active"
                                            aria-current="page"
                                        >
                                            <%= namaBulan %>
                                        </li>
                                        <li
                                            class="breadcrumb-item active"
                                            aria-current="page"
                                        >
                                            <%= nama %>
                                        </li>
                                    </ol>
                                </nav>
                            </div>
                        </div>
                    </div>
                    <div class="card">
                        <div class="table-responsive">
                            <table class="table table-lg text-center">
                                <thead>
                                    <tr>
                                        <th>Jenis</th>
                                        <th>Jumlah</th>
                                        <th>Rate</th>
                                        <th>Total</th>
                                    </tr>
                                </thead>
                                <form
                                    action="/cetakSlip/<%= bulan %>/<%= nama %>"
                                    method="post"
                                >
                                    <tbody>
                                        <tr>
                                            <td class="text-bold-500">
                                                Jumlah Masuk (transport)
                                            </td>
                                            <td>
                                                <%= person[0].jumlah_masuk %>
                                            </td>
                                            <td><%= rate[0].transport %></td>
                                            <td>
                                                <%= formatAngka(gaji.transport)
                                                %>
                                            </td>
                                        </tr>

                                        <tr>
                                            <td class="text-bold-500">
                                                Jumlah siswa
                                            </td>
                                            <td><%= person[0].siswa %></td>
                                            <td><%= rate[0].perSiswa %></td>
                                            <td>
                                                <%= formatAngka(gaji.perSiswa)
                                                %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td class="text-bold-500">
                                                Jam Mengajar
                                            </td>
                                            <td><%= person[0].jam_kerja %></td>
                                            <td><%= rate[0].perJam %></td>
                                            <td>
                                                <%= formatAngka(gaji.perJam) %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td class="text-bold-500">
                                                Halaman Ketik
                                            </td>
                                            <td>
                                                <%= person[0].halaman_ketik %>
                                            </td>
                                            <td><%= rate[0].hlmnKetik%></td>
                                            <td>
                                                <%= formatAngka(gaji.hlmnKetik)
                                                %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td class="text-bold-500">
                                                Halaman Kunci
                                            </td>
                                            <td>
                                                <%= person[0].halaman_kunci%>
                                            </td>
                                            <td><%= rate[0].hlmnKunci %></td>
                                            <td>
                                                <%= formatAngka(gaji.hlmnKunci)
                                                %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td class="text-bold-500">
                                                Quiziz
                                            </td>
                                            <td><%= person[0].quiziz %></td>
                                            <td><%= rate[0].hlmnQuiziz%></td>
                                            <td>
                                                <%= formatAngka(gaji.hlmnQuiziz)
                                                %>
                                            </td>
                                        </tr>

                                        <tr>
                                            <td class="text-bold-500">Total</td>
                                            <td></td>
                                            <td></td>
                                            <td>
                                                <input
                                                    id="totalGaji"
                                                    type="text"
                                                    value="<%= formatAngka(gaji.total) %>"
                                                    readonly
                                                    name="total"
                                                />
                                            </td>
                                        </tr>
                                        <tr>
                                            <td
                                                class="text-bold-500 text-danger"
                                            >
                                                Kinerja
                                            </td>
                                            <td></td>
                                            <td></td>
                                            <td>
                                                <div class="input-group">
                                                    <button
                                                        class="btn btn-primary"
                                                        type="button"
                                                        id="buttonKinerja"
                                                    >
                                                        Add
                                                    </button>
                                                    <input
                                                        type="number"
                                                        class="form-control"
                                                        id="addKinerja"
                                                        placeholder="example: 100000"
                                                        aria-label="Example text with button addon"
                                                        aria-describedby="button-addon1"
                                                        name="kinerja"
                                                        required
                                                    />
                                                </div>
                                            </td>
                                        </tr>
                                        <!-- <tr>
                                            <td
                                                class="text-bold-500 text-success"
                                            >
                                                Terbayarkan
                                            </td>
                                            <td></td>
                                            <td></td>
                                            <td>
                                                <div class="input-group">
                                                    <button
                                                        class="btn btn-primary"
                                                        type="button"
                                                        id="terbayar"
                                                    >
                                                        Add
                                                    </button>
                                                    <input
                                                        type="text"
                                                        class="form-control"
                                                        id="addBayar"
                                                        name="terbayar"
                                                        placeholder="example: 100000"
                                                        aria-label="Example text with button addon"
                                                        aria-describedby="button-addon1"
                                                        required
                                                    />
                                                </div>
                                            </td>
                                        </tr> -->

                                        <tr>
                                            <td></td>
                                            <td></td>
                                            <td></td>
                                            <td>
                                                <button
                                                    type="submit"
                                                    class="btn btn-danger"
                                                >
                                                    Cetak Slip Gaji
                                                </button>
                                            </td>
                                        </tr>
                                        <tr></tr>
                                    </tbody>
                                </form>
                            </table>
                        </div>
                    </div>
                </div>
                <%- include('./partials/footer.ejs') %>
            </div>
        </div>

        <script src="/assets/static/js/components/dark.js"></script>
        <script src="/assets/extensions/perfect-scrollbar/perfect-scrollbar.min.js"></script>
        <script src="/assets/compiled/js/app.js"></script>
        <script src="/js/Add.js"></script>
    </body>
</html>
